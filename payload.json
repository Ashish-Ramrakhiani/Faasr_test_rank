{
    "ComputeServers": {
        "My_GitHub_Account": {
            "FaaSType": "GitHubActions",
            "UserName": "Ashish-Ramrakhiani",
            "ActionRepoName": "Faasr_test_rank_actions",
            "Branch": "main"
        }
    },
    "DataStores": {
        "My_Minio_Bucket": {
            "Endpoint": "https://s3.faasr.io",
            "Bucket": "bucket5",
            "Writable": "TRUE"
        }
    },
    "FunctionList": {
        "start": {
            "FunctionName": "create_sample_data",
            "FaaSServer": "My_GitHub_Account",
            "Arguments": {
                "folder": "tutorial",
                "output1": "sample1.csv",
                "output2": "sample2.csv"
            },
            "InvokeNext": "sum(4)"
        },
        "sum": {
            "FunctionName": "compute_sum",
            "FaaSServer": "My_GitHub_Account",
            "Arguments": {
                "folder": "tutorial",
                "input1": "sample1.csv",
                "input2": "sample2.csv",
                "output": "sum.csv"
            },
            "InvokeNext": "end"
        },
        "end": {
            "FunctionName": "verify_rank_files",
            "FaaSServer": "My_GitHub_Account",
            "Arguments": {
                "folder": "tutorial2",
                "expected_rank": "4"
            }
        }
    },
     "ActionContainers": {
        "start": "ghcr.io/faasr/github-actions-tidyverse:1.4.0",
        "sum": "ghcr.io/faasr/github-actions-tidyverse:1.4.0",
        "end": "ghcr.io/faasr/github-actions-tidyverse:1.4.0"
    },
    "FunctionGitRepo": {
        "create_sample_data": "Ashish-Ramrakhiani/Faasr_test_rank",
        "compute_sum": "Ashish-Ramrakhiani/Faasr_test_rank",
        "verify_rank_files": "Ashish-Ramrakhiani/Faasr_test_rank"
    },
    "FunctionInvoke": "start",
    "FaaSrLog": "FaaSrLogrank",
    "LoggingDataStore": "My_Minio_Bucket",
    "DefaultDataStore": "My_Minio_Bucket"
}

